LOG_LEVEL="INFO"
PYTHON_ENV="dev"
OPENAI_API_KEY="YOUR_OPENAI_API_KEY"
QDRANT_COLLECTION_NAME="ragify"

# ===== LANGSMITH TRACING CREDENTIALS =====
LANGSMITH_TRACING="true"
LANGSMITH_ENDPOINT="https://api.smith.langchain.com"
LANGSMITH_API_KEY="YOUR_LANGSMITH_API_KEY"
LANGSMITH_PROJECT="Ragify"

# ===== PRODUCTION CREDENTIALS (not needed if PYTHON_ENV is dev) =====
MONGODB_URI="mongodb+srv://username:password@your_mongodb_cluster.mongodb.net/?retryWrites=true&w=majority&appName=YourAppName"
AWS_ACCESS_KEY_ID="YOUR_AWS_ACCESS_KEY_ID"
AWS_SECRET_ACCESS_KEY="YOUR_AWS_SECRET_ACCESS_KEY"
AWS_REGION="us-east-2"
AWS_BUCKET_NAME="your_aws_bucket_name"
QDRANT_URL="https://your-qdrant-url.com"
QDRANT_API_KEY="YOUR_QDRANT_API_KEY"
